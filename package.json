{
  "name": "react-express-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@tinymce/tinymce-react": "^3.6.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "merge": ">=2.1.1",
    "mongoose": "^5.10.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-table-v6": "^6.8.6",
    "sweetalert-react": "^0.4.11"
  },
  "scripts": {
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "nodemon lib/server.js --ignore src/",
    "dev": "react-scripts start",
    "start": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run client\"",
    "seeds": "node lib/seeds/index.js",
    "heroku-postbuild": "cd src && yarn install && yarn run build"
  },
  "proxy": "http://localhost:3050",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}